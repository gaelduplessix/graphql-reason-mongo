    "refmterr": "*",
    "@opam/graphql-lwt": "*",
    "@opam/mongo": "*",
    "@opam/bson": "*"
  },
  "resolutions": {
    "@esy-ocaml/reason": "anmonteiro/reason#anmonteiro/dune-1.4",
    "@opam/mongo": "anmonteiro/mongo#dunify",
    "@opam/bson": "gaelduplessix/bson:bson.opam#fix-object-id-decoding"
